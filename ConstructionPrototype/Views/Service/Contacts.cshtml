@{
    ViewData["Title"] = "Contacts";
}


<h1 class="text-center">@ViewData["Title"]</h1>
<p  class="text-center"><em>You got some questions?</em></p>
<p  class="text-center"><em>Let us know!</em></p>


    
<div class="row row-gutter">
    <div class="row row-gutter">
        <div class="col-sm-12 form-group">
            <input class="form-control" id="name" name="name" placeholder="Name" type="text" required margin-left="20">
        </div>
        <div class="col-sm-12 form-group">
            <input class="form-control" id="email" name="email" placeholder="Email" type="email" required>
        </div>
    </div>

    <textarea class="form-control" id="comments" name="comments" placeholder="Write to us here." rows="5"></textarea>

    <div class="row row-gutter">
        <div class="col-md-12 form-group">
            <button class="btn btn-primary" type="submit">Send email</button>
        </div>
    </div>
</div>



@* v-----------CSS-----------v *@ 

<div class="mapouter row-map">
        <div class="gmap_canvas" position: relative>
            <iframe width="100%" 
                height="450" 
                src="https://maps.google.com/maps?q=85-162%20bu%C5%82garska%203&t=&z=15&ie=UTF8&iwloc=&output=embed" 
                frameborder="0" 
                scrolling="no" 
                marginheight="0" 
                marginwidth="0">
            </iframe>
        </div>
</div>

